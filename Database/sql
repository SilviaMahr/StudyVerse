CREATE TABLE lvas (
    id SERIAL PRIMARY KEY,
    hierarchielevel0 TEXT,
    hierarchielevel1 TEXT,
    hierarchielevel2 TEXT,
    type VARCHAR(5),
    name TEXT,
    ects INTEGER
);


